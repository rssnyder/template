{
  "containerDefinitions": [
    {
      "portMappings": [],
      "cpu": 1,
      "environment": [
        {
          "name": "ACCOUNT_ID",
          "value": "<+account.identifier>"
        },
        {
          "name": "DELEGATE_TOKEN",
          "value": "<+account.identifier>"
        },
        {
          "name": "DELEGATE_TYPE",
          "value": "DOCKER"
        },
        {
          "name": "INIT_SCRIPT",
          "value": ""
        },
        {
          "name": "DEPLOY_MODE",
          "value": "KUBERNETES"
        },
        {
          "name": "MANAGER_HOST_AND_PORT",
          "value": "<+serviceVariables.manager_host_and_port>"
        },
        {
          "name": "DELEGATE_NAME",
          "value": "<+serviceVariables.delegate_name>"
        },
       {
          "name": "LOG_STREAMING_SERVICE_URL",
          "value": "<+serviceVariables.log_streaming_Service_url>"
        },
       {
          "name": "DELEGATE_TAGS",
          "value": ""
        },

        {
          "name": "NEXT_GEN",
          "value": "true"
        }
      ],
      "memory": "<+serviceVariables.memory>",
      "image": "<+artifact.image>",
      "essential": true,
      "hostname": "<+serviceVariables.delegate_name>",
      "name": "<+serviceVariables.delegate_name>"
    }
  ],
  "memory": "<+serviceVariables.memory>",
  "requiresCompatibilities": [
    "EC2"
  ],
  
  "cpu": "<+serviceVariables.cpu>",
  "family": "harness-delegate-task-spec"
}
